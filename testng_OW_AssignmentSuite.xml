<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Open Weather" verbose="2" parallel="methods" thread-count="1" data-provider-thread-count="4">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
    <test verbose="2" name="NAB Assignment Suite" >
        <classes>
            <class name="com.openweather.testsuites.AssignmentSuite" />
        </classes>
    </test>
</suite>